<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude sensitive data from backup -->
    <exclude domain="sharedpref" path="auth_prefs.xml"/>
    <exclude domain="sharedpref" path="user_data.xml"/>
    <exclude domain="database" path="secure_db.db"/>
    <exclude domain="file" path="private/"/>
    <exclude domain="file" path="cache/"/>
    <exclude domain="file" path="temp/"/>
</full-backup-content>
